{
  "compilerOptions": {
    "declaration": true,
    "target": "es6",
    "module": "CommonJS",
    "moduleResolution": "nodenext",
    "strict": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "outDir": "dist"
  },
  "include": ["addresses", "types"]
}
